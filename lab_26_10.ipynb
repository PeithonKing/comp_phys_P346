    "def forward_euler_differential_equation(f, x0, y0, h, n):\n",
    "\tx = x0\n",
    "\ty = y0\n",
    "\tfor _ in range(n):\n",
    "\t\ty += h*f(x, y)\n",
    "\t\tx += h\n",
    "\treturn y"
